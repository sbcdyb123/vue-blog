{
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "~/*": ["./*"],
        "@/*": ["./*"],
        "~~/*": ["./*"],
        "@@/*": ["./*"]
      },
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}